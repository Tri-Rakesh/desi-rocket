{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "desi-rocket/*": ["./src/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
